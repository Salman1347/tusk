package StringImmExample;

import java.util.ArrayList;

public class AllTypeOFList {

	public static void main(String[] args) {
		
		ArrayList<String> ListOfCities = new ArrayList<>();
		ListOfCities.add("Bangalore");
		
		ListOfCities.add("Lucknow");
		ListOfCities.add("Hyderabad");
		ListOfCities.add("Rajasthan");
		ListOfCities.add("Mysore");
		
		System.out.println(ListOfCities.get(2));
		
		//search a city
		
		for(String str:ListOfCities) {
			
			if(str.equals("Bangalore")) {
				
				System.out.println(str);
				
			}
		}
		

	}

}
